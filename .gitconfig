[user]
	name = Dave Fergemann
	email = fergemann@gmail.com
[core]
	editor = vim
	autocrlf = input
	safecrlf = true
	excludesfile = /Users/David_Fergemann/.gitignore_global
[alias]
	co = checkout
	ci = commmit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
